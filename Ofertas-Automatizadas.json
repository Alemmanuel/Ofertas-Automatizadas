        "jsCode": "const rows = items.map((item, idx) => `\n  <tr>\n    <td style=\\\"padding:6px 12px;border:1px solid #ccc\\\">${idx+1}</td>\n    <td style=\\\"padding:6px 12px;border:1px solid #ccc\\\">${item.json.Empresa}</td>\n    <td style=\\\"padding:6px 12px;border:1px solid #ccc\\\">${item.json.Cargo}</td>\n    <td style=\\\"padding:6px 12px;border:1px solid #ccc\\\"><a href=\\\"${item.json.URL}\\\" target=\\\"_blank\\\">Ver oferta</a></td>\n  </tr>\n`).join('');\nconst html = `\n  <div style=\\\"font-family:Arial,sans-serif;max-width:600px;margin:auto\\\">\n    <h2 style=\\\"background:#1976d2;color:#fff;padding:12px 0;text-align:center\\\">Postulaciones Freelance</h2>\n    <table style=\\\"border-collapse:collapse;width:100%\\\">\n      <thead>\n        <tr style=\\\"background:#f5f5f5\\\">\n          <th style=\\\"padding:8px;border:1px solid #ccc\\\">#</th>\n          <th style=\\\"padding:8px;border:1px solid #ccc\\\">Empresa</th>\n          <th style=\\\"padding:8px;border:1px solid #ccc\\\">Cargo</th>\n          <th style=\\\"padding:8px;border:1px solid #ccc\\\">Enlace</th>\n        </tr>\n      </thead>\n      <tbody>\n        ${rows}\n      </tbody>\n    </table>\n    <p style=\\\"margin-top:20px;font-size:13px;color:#888\\\">Enviado autom√°ticamente por n8n</p>\n  </div>\n`;\nreturn [{ json: { html } }];\n"
